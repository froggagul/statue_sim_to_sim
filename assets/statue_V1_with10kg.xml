  <actuator>
    <!-- ===== Left leg ===== -->
    <!-- <motor name="left_hip_pitch_m" joint="left_hip_pitch_joint" ctrllimited="true" ctrlrange="-300 300"/>
    <motor name="left_hip_roll_m" joint="left_hip_roll_joint" ctrllimited="true" ctrlrange="-300 300"/>
    <motor name="left_hip_yaw_m" joint="left_hip_yaw_joint" ctrllimited="true" ctrlrange="-250 250"/>
    <motor name="left_knee_pitch_m" joint="left_knee_pitch_joint" ctrllimited="true" ctrlrange="-350 350"/>
    <motor name="left_ankle_pitch_m" joint="left_ankle_pitch_joint" ctrllimited="true" ctrlrange="-200 200"/>
    <motor name="left_ankle_roll_m" joint="left_ankle_roll_joint" ctrllimited="true" ctrlrange="-200 200"/> -->
    <position name="left_hip_pitch_m" joint="left_hip_pitch_joint" ctrllimited="true" kp="150" kv="5" ctrlrange="-140 140"/>
    <position name="left_hip_roll_m" joint="left_hip_roll_joint" ctrllimited="true" kp="150" kv="5" ctrlrange="-140 140"/>
    <position name="left_hip_yaw_m" joint="left_hip_yaw_joint" ctrllimited="true" kp="200" kv="5" ctrlrange="-140 140"/>
    <position name="left_knee_pitch_m" joint="left_knee_pitch_joint" ctrllimited="true" kp="200" kv="5" ctrlrange="-140 140"/>
    <position name="left_ankle_pitch_m" joint="left_ankle_pitch_joint" ctrllimited="true" kp="50" kv="4" ctrlrange="-140 140"/>
    <position name="left_ankle_roll_m" joint="left_ankle_roll_joint" ctrllimited="true" kp="50" kv="4" ctrlrange="-140 140"/>

    <!-- ===== Right leg ===== -->
    <!-- <motor name="right_hip_pitch_m" joint="right_hip_pitch_joint" ctrllimited="true" ctrlrange="-300 300"/>
    <motor name="right_hip_roll_m" joint="right_hip_roll_joint" ctrllimited="true" ctrlrange="-300 300"/>
    <motor name="right_hip_yaw_m" joint="right_hip_yaw_joint" ctrllimited="true" ctrlrange="-250 250"/>
    <motor name="right_knee_pitch_m" joint="right_knee_pitch_joint" ctrllimited="true" ctrlrange="-350 350"/>
    <motor name="right_ankle_pitch_m" joint="right_ankle_pitch_joint" ctrllimited="true" ctrlrange="-200 200"/>
    <motor name="right_ankle_roll_m" joint="right_ankle_roll_joint" ctrllimited="true" ctrlrange="-200 200"/> -->
    <position name="right_hip_pitch_m" joint="right_hip_pitch_joint" ctrllimited="true" kp="150" kv="5" ctrlrange="-140 140"/>
    <position name="right_hip_roll_m" joint="right_hip_roll_joint" ctrllimited="true" kp="150" kv="5" ctrlrange="-140 140"/>
    <position name="right_hip_yaw_m" joint="right_hip_yaw_joint" ctrllimited="true" kp="200" kv="5" ctrlrange="-140 140"/>
    <position name="right_knee_pitch_m" joint="right_knee_pitch_joint" ctrllimited="true" kp="200" kv="5" ctrlrange="-140 140"/>
    <position name="right_ankle_pitch_m" joint="right_ankle_pitch_joint" ctrllimited="true" kp="50" kv="4" ctrlrange="-140 140"/>
    <position name="right_ankle_roll_m" joint="right_ankle_roll_joint" ctrllimited="true" kp="50" kv="4" ctrlrange="-140 140"/>
  </actuator>

  <contact>
    <exclude body1="pelvis_link" body2="left_hip_pitch_link" />
    <exclude body1="pelvis_link" body2="right_hip_pitch_link" />

    <!-- Left leg parent-child -->
    <exclude body1="left_hip_pitch_link" body2="left_hip_roll_link" />
    <exclude body1="left_hip_roll_link"  body2="left_hip_yaw_link" />
    <exclude body1="left_hip_yaw_link"   body2="left_knee_pitch_link" />
    <exclude body1="left_knee_pitch_link" body2="left_ankle_pitch_link" />
    <exclude body1="left_ankle_pitch_link" body2="left_ankle_roll_link" />

    <!-- Right leg parent-child -->
    <exclude body1="right_hip_pitch_link" body2="right_hip_roll_link" />
    <exclude body1="right_hip_roll_link"  body2="right_hip_yaw_link" />
    <exclude body1="right_hip_yaw_link"   body2="right_knee_pitch_link" />
    <exclude body1="right_knee_pitch_link" body2="right_ankle_pitch_link" />
    <exclude body1="right_ankle_pitch_link" body2="right_ankle_roll_link" />
  </contact>

  <sensor>
    <gyro name="gyro" site="imu"/>
    <accelerometer name="accelerometer" site="imu"/>
  </sensor>

</mujoco>
