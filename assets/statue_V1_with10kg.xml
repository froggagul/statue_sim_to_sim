      <joint name="root_free" type="free"/>
      <geom name="pelvis_link_collision" type="mesh" rgba="0.75294 0.75294 0.75294 1" mesh="pelvis_with_10kg" class="collision"/>
      <body name="left_hip_pitch_link" pos="0 0.089613 -0.026765">
        <inertial pos="0.00120687 0.0321919 -0.022126" quat="0.648667 0.314246 0.620766 0.308431" mass="0.59" diaginertia="0.00147738 0.00117486 0.000975686"/>
