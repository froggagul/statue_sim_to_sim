                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
      <body name="right_hip_pitch_link" pos="0 -0.089614 -0.026765">
        <inertial pos="0.00120686 -0.0321919 -0.0221259" quat="0.308431 0.620766 0.314246 0.648667" mass="0.59" diaginertia="0.00147738 0.00117486 0.000975686"/>
