<mujoco model="statue_V1">
  <option gravity="0 0 -9.81" />
  <default>                                       <!-- root default 시작 -->
    <!--
    <joint range="-6.28319 6.28319" armature="0.05" damping="0.01" frictionloss="0.01"/>
    <position ctrlrange="-6.2831 6.2831" />
    -->


    <default class="robot">                       <!-- robot-class 기본값 -->
      <default class="motor">                     <!-- motor-class 기본값 -->
        <joint range="-6.28319 6.28319"
              armature="0.1"
              damping="0.1"
              frictionloss="0.01"/>
        <motor />                                 <!-- motor 태그의 기본값 (속성 없이) -->
      </default>

      <default class="visual">                    <!-- visual-class 기본값 -->
        <geom material="visualgeom"
              contype="0"
              conaffinity="0"
              group="2" />
      </default>

      <default class="collision">
        <geom material="gray_rgba"
              condim="3"
              contype="0"
              conaffinity="1"
              priority="1"
              group="1"
              solimp="0.9 0.99 0.001"
              solref="0.002 1"
              friction="1.2 0.02 0.002" />
      </default>
    </default>                                    <!-- robot-class 닫기 -->
  </default>   

  <compiler angle="radian" autolimits="true" meshdir="./meshes"/>

  <asset>
    <material name="none" rgba="0.79216 0.81961 0.93333 1" />
    <material name="default_material" rgba="0.7 0.7 0.7 1" />
    <material name="collision_material" rgba="1.0 0.28 0.1 0.9" />
    <material name="gray_rgba" rgba="0.7 0.7 0.7 1.0" />
    <mesh name="pelvis_with_10kg" file="pelvis_with_10kg.STL"/>
    <mesh name="left_hip_pitch_link" file="left_hip_pitch_link.STL"/>
    <mesh name="left_hip_roll_link" file="left_hip_roll_link.STL"/>
